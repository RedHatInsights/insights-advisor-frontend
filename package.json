    "git-revision-webpack-plugin": "3.0.3",
    "glob": "7.1.3",
    "html-replace-webpack-plugin": "2.5.3",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "koa-connect": "2.0.1",
    "lodash-webpack-plugin": "0.11.5",
    "material-ui": "0.20.2",
    "mini-css-extract-plugin": "0.4.4",
