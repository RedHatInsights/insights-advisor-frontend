{
  "name": "insights-frontend",
  "version": "1.0.2",
  "private": false,
  "author": "Red Hat",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/RedHatInsights/insights-advisor-frontend.git"
  },
  "dependencies": {
    "@babel/runtime": "7.6.3",
    "@patternfly/react-charts": "5.0.13",
    "@patternfly/react-core": "3.112.3",
    "@patternfly/react-icons": "3.14.12",
    "@patternfly/react-table": "2.22.22",
    "@patternfly/react-tokens": "2.6.31",
    "@redhat-cloud-services/frontend-components": "0.0.27",
    "@redhat-cloud-services/frontend-components-inventory-insights": "0.1.19",
    "@redhat-cloud-services/frontend-components-notifications": "0.0.7",
    "@redhat-cloud-services/frontend-components-remediations": "0.0.3",
    "@redhat-cloud-services/frontend-components-translations": "0.0.6",
    "@redhat-cloud-services/frontend-components-utilities": "0.0.9",
    "babel-plugin-react-intl": "4.1.20",
    "classnames": "2.2.6",
    "react": "16.10.2",
    "react-content-loader": "4.3.2",
    "react-dom": "16.10.2",
    "react-intl": "3.3.2",
    "react-markdown": "4.2.2",
    "react-redux": "5.1.0",
    "react-router-dom": "5.1.2",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-async-to-generator": "7.5.0",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.6.3",
    "@redhat-cloud-services/frontend-components-config": "0.0.9",
    "axios": "0.19.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-dual-import": "1.2.1",
    "babel-plugin-lodash": "3.3.4",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "enzyme-to-json": "3.4.2",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.1.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "moment": "2.24.0",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "react-test-renderer": "16.10.2",
    "seamless-immutable": "7.1.4",
    "stylelint": "11.1.1",
    "stylelint-config-recommended-scss": "4.0.0",
    "stylelint-scss": "3.11.1",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.5.2",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2"
  },
  "scripts": {
    "build": "webpack --config config/prod.webpack.config.js",
    "test": "jest --passWithNoTests",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "prod": "NODE_ENV=production webpack-dev-server --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "webpack-dev-server --config config/dev.webpack.config.js",
    "travis:build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "travis:verify": "npm-run-all travis:build lint test",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run travis:verify"
  },
  "insights": {
    "appname": "insights"
  }
}
