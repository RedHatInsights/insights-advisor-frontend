    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "16.6.0",
    "react-timeago": "4.1.9",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "5.1.1",
    "regenerator-runtime": "0.12.1",
    "sass-loader": "7.1.0",
    "seamless-immutable": "7.1.4",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "stylelint": "9.6.0",
    "stylelint-config-recommended-scss": "3.2.0",
